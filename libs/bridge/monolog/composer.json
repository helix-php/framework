{
    "name": "helix/monolog-bridge",
    "type": "library",
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "monolog/monolog": "^2.3"
    },
    "autoload": {
        "psr-4": {
            "Helix\\Bridge\\Monolog\\": "src"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5.20",
        "vimeo/psalm": "^4.22"
    },
    "autoload-dev": {
        "psr-4": {
            "Helix\\Bridge\\Monolog\\Tests\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev",
            "dev-main": "1.x-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
