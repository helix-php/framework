{
    "name": "helix/doctrine-bridge",
    "type": "library",
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "helix/pool": "^1.0",
        "doctrine/orm": "^2.11",
        "happyr/doctrine-specification": "^1.1"
    },
    "autoload": {
        "psr-4": {
            "Helix\\Bridge\\Doctrine\\": "src"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5.20",
        "vimeo/psalm": "^4.22"
    },
    "autoload-dev": {
        "psr-4": {
            "Helix\\Bridge\\Doctrine\\Tests\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev",
            "dev-main": "1.x-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
