{
    "name": "helix/http-cookie",
    "type": "library",
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "psr/clock": "^1.0",
        "helix/http-header": "^1.0",
        "helix/http-cookie-contracts": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Helix\\Http\\Cookie\\": "src"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^10.4",
        "vimeo/psalm": "^5.15"
    },
    "autoload-dev": {
        "psr-4": {
            "Helix\\Http\\Cookie\\Tests\\": "tests"
        }
    },
    "provide": {
        "helix/http-cookie-contracts-implementation": "^1.0"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev",
            "dev-main": "1.x-dev"
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
