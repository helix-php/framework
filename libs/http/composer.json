{
    "name": "helix/http",
    "type": "library",
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "ext-json": "*",
        "psr/http-message": "^1.0",
        "psr/http-message-implementation": "^1.0",
        "psr/http-factory": "^1.0",
        "psr/http-factory-implementation": "^1.0",
        "helix/http-cookie": "^1.0",
        "helix/http-header": "^1.0",
        "helix/http-method": "^1.0",
        "helix/http-status-code": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Helix\\Http\\": "src"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5.20",
        "vimeo/psalm": "^5.6"
    },
    "autoload-dev": {
        "psr-4": {
            "Helix\\Http\\Tests\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev",
            "dev-main": "1.x-dev"
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
