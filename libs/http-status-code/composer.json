{
    "name": "helix/http-status-code",
    "type": "library",
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "helix/http-status-code-contracts": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Helix\\Http\\StatusCode\\": "src"
        }
    },
    "require-dev": {
        "nyholm/psr7": "^1.5",
        "symfony/http-foundation": "^5.4|^6.0",
        "laminas/laminas-diactoros": "^2.12",
        "httpsoft/http-message": "^1.0",
        "phpunit/phpunit": "^9.5.20",
        "vimeo/psalm": "^5.6"
    },
    "autoload-dev": {
        "psr-4": {
            "Helix\\Http\\StatusCode\\Tests\\": "tests"
        }
    },
    "provide": {
        "helix/http-status-code-contracts-implementation": "^1.0"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev",
            "dev-main": "1.x-dev"
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
