{
    "name": "helix/framework",
    "license": "MIT",
    "type": "library",
    "description": "Helix Framework",
    "homepage": "https://github.com/helix-php",
    "authors": [
        {
            "name": "Kirill Nesmeyanov",
            "email": "nesk@xakep.ru",
            "role": "maintainer"
        }
    ],
    "require": {
        "php": "^8.1",
        "ext-json": "*",
        "ext-mbstring": "*",
        "ext-pdo": "*",
        "ext-session": "*",
        "cache/array-adapter": "^1.2",
        "cache/filesystem-adapter": "^1.2",
        "doctrine/orm": "^2.11",
        "filp/whoops": "^2.14",
        "happyr/doctrine-specification": "^1.1",
        "monolog/monolog": "^2.3",
        "nikic/fast-route": "^1.3",
        "nyholm/psr7": "^1.5",
        "phpdocumentor/reflection-docblock": "^5.0",
        "psr/container": "^1.1|^2.0",
        "psr/event-dispatcher": "^1.0|^2.0|^3.0",
        "psr/http-factory": "^1.0",
        "psr/http-factory-implementation": "^1.0",
        "psr/http-message": "^1.0",
        "psr/http-message-implementation": "^1.0",
        "psr/http-server-middleware": "^1.0",
        "psr/log": "^1.0|^2.0|^3.0",
        "ramsey/uuid": "^4.3",
        "symfony/console": "^5.4|^6.0",
        "symfony/process": "^5.4|^6.0",
        "symfony/yaml": "^5.4|^6.0",
        "twig/twig": "^3.3",
        "vlucas/phpdotenv": "^5.4"
    },
    "autoload": {
        "exclude-from-classmap": [
            "libs/mime/src/Script"
        ],
        "files": [
            "libs/clock/src/polyfill.php",
            "libs/env/src/helpers.php"
        ],
        "psr-4": {
            "Helix\\Boot\\": "libs/boot/src",
            "Helix\\Bridge\\Cache\\": "libs/bridge/cache/src",
            "Helix\\Bridge\\Doctrine\\": "libs/bridge/doctrine/src",
            "Helix\\Bridge\\Monolog\\": "libs/bridge/monolog/src",
            "Helix\\Bridge\\Twig\\": "libs/bridge/twig/src",
            "Helix\\Clock\\": "libs/clock/src",
            "Helix\\Config\\": "libs/config/src",
            "Helix\\Container\\": "libs/container/src",
            "Helix\\Contracts\\Arrayable\\": "libs/contracts/arrayable/src",
            "Helix\\Contracts\\Container\\": "libs/contracts/container/src",
            "Helix\\Contracts\\ErrorHandler\\": "libs/contracts/error-handler/src",
            "Helix\\Contracts\\ErrorHandler\\Http\\": "libs/contracts/error-handler-http/src",
            "Helix\\Contracts\\EventDispatcher\\": "libs/contracts/event-dispatcher/src",
            "Helix\\Contracts\\Http\\Header\\": "libs/contracts/http-header/src",
            "Helix\\Contracts\\Http\\Method\\": "libs/contracts/http-method/src",
            "Helix\\Contracts\\Http\\Middleware\\": "libs/contracts/http-middleware/src",
            "Helix\\Contracts\\Http\\StatusCode\\": "libs/contracts/http-status-code/src",
            "Helix\\Contracts\\Memoizable\\": "libs/contracts/memoizable/src",
            "Helix\\Contracts\\Mime\\": "libs/contracts/mime/src",
            "Helix\\Contracts\\ParamResolver\\": "libs/contracts/param-resolver/src",
            "Helix\\Contracts\\Router\\": "libs/contracts/router/src",
            "Helix\\Contracts\\View\\": "libs/contracts/view/src",
            "Helix\\Debug\\": "libs/debug/src",
            "Helix\\Env\\": "libs/env/src",
            "Helix\\EventDispatcher\\": "libs/event-dispatcher/src",
            "Helix\\Foundation\\": "libs/foundation/src",
            "Helix\\Foundation\\Console\\": "libs/foundation-console/src",
            "Helix\\Foundation\\Http\\": "libs/foundation-http/src",
            "Helix\\Http\\": "libs/http/src",
            "Helix\\Http\\Header\\": "libs/http-header/src",
            "Helix\\Http\\Method\\": "libs/http-method/src",
            "Helix\\Http\\Middleware\\": "libs/http-middleware/src",
            "Helix\\Http\\StatusCode\\": "libs/http-status-code/src",
            "Helix\\Mime\\": "libs/mime/src",
            "Helix\\ParamInfo\\": "libs/param-info/src",
            "Helix\\ParamResolver\\": "libs/param-resolver/src",
            "Helix\\Pool\\": "libs/pool/src",
            "Helix\\Router\\": "libs/router/src",
            "Helix\\Router\\Generator\\": "libs/router-generator/src",
            "Helix\\Server\\": "libs/server/src",
            "Helix\\Server\\Sapi\\": "libs/server-sapi/src",
            "Helix\\Session\\": "libs/session/src",
            "Helix\\Session\\Http\\": "libs/session-http/src",
            "Helix\\View\\": "libs/view/src"
        }
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.8",
        "httpsoft/http-message": "^1.0",
        "jetbrains/phpstorm-attributes": "^1.0",
        "laminas/laminas-diactoros": "^2.12",
        "phpbench/phpbench": "^1.0",
        "phpunit/phpunit": "^9.5.20",
        "stella-maris/clock": "^0.1",
        "symfony/http-foundation": "^5.4|^6.0",
        "symfony/var-dumper": "^5.4|^6.0",
        "symplify/monorepo-builder": "^10.0",
        "vimeo/psalm": "^4.22"
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests",
            "Helix\\Boot\\Tests\\": "libs/boot/tests",
            "Helix\\Bridge\\Cache\\Tests\\": "libs/bridge/cache/tests",
            "Helix\\Bridge\\Doctrine\\Tests\\": "libs/bridge/doctrine/tests",
            "Helix\\Bridge\\Monolog\\Tests\\": "libs/bridge/monolog/tests",
            "Helix\\Bridge\\Twig\\Tests\\": "libs/bridge/twig/tests",
            "Helix\\Clock\\Tests\\": "libs/clock/tests",
            "Helix\\Config\\Tests\\": "libs/config/tests",
            "Helix\\Container\\Tests\\": "libs/container/tests",
            "Helix\\Debug\\Tests\\": "libs/debug/tests",
            "Helix\\Env\\Tests\\": "libs/env/tests",
            "Helix\\EventDispatcher\\Tests\\": "libs/event-dispatcher/tests",
            "Helix\\Foundation\\Console\\Tests\\": "libs/foundation-console/tests",
            "Helix\\Foundation\\Http\\Tests\\": "libs/foundation-http/tests",
            "Helix\\Foundation\\Tests\\": "libs/foundation/tests",
            "Helix\\Http\\Header\\Tests\\": "libs/http-header/tests",
            "Helix\\Http\\Method\\Tests\\": "libs/http-method/tests",
            "Helix\\Http\\Middleware\\Tests\\": "libs/http-middleware/tests",
            "Helix\\Http\\StatusCode\\Tests\\": "libs/http-status-code/tests",
            "Helix\\Http\\Tests\\": "libs/http/tests",
            "Helix\\Mime\\Tests\\": "libs/mime/tests",
            "Helix\\ParamInfo\\Tests\\": "libs/param-info/tests",
            "Helix\\ParamResolver\\Tests\\": "libs/param-resolver/tests",
            "Helix\\Pool\\Tests\\": "libs/pool/tests",
            "Helix\\Router\\Generator\\Tests\\": "libs/router-generator/tests",
            "Helix\\Router\\Tests\\": "libs/router/tests",
            "Helix\\Server\\Sapi\\Tests\\": "libs/server-sapi/tests",
            "Helix\\Server\\Tests\\": "libs/server/tests",
            "Helix\\Session\\Http\\Tests\\": "libs/session-http/tests",
            "Helix\\Session\\Tests\\": "libs/session/tests",
            "Helix\\View\\Tests\\": "libs/view/tests"
        }
    },
    "replace": {
        "helix/arrayable-contracts": "self.version",
        "helix/boot": "self.version",
        "helix/cache-bridge": "self.version",
        "helix/clock": "self.version",
        "helix/config": "self.version",
        "helix/container": "self.version",
        "helix/container-contracts": "self.version",
        "helix/debug": "self.version",
        "helix/doctrine-bridge": "self.version",
        "helix/env": "self.version",
        "helix/error-handler-contracts": "self.version",
        "helix/error-handler-http-contracts": "self.version",
        "helix/event-dispatcher": "self.version",
        "helix/event-dispatcher-contracts": "self.version",
        "helix/foundation": "self.version",
        "helix/foundation-console": "self.version",
        "helix/foundation-http": "self.version",
        "helix/http": "self.version",
        "helix/http-header": "self.version",
        "helix/http-header-contracts": "self.version",
        "helix/http-method": "self.version",
        "helix/http-method-contracts": "self.version",
        "helix/http-middleware": "self.version",
        "helix/http-middleware-contracts": "self.version",
        "helix/http-status-code": "self.version",
        "helix/http-status-code-contracts": "self.version",
        "helix/memoizable-contracts": "self.version",
        "helix/mime": "self.version",
        "helix/mime-contracts": "self.version",
        "helix/monolog-bridge": "self.version",
        "helix/param-info": "self.version",
        "helix/param-resolver": "self.version",
        "helix/param-resolver-contracts": "self.version",
        "helix/pool": "self.version",
        "helix/router": "self.version",
        "helix/router-contracts": "self.version",
        "helix/router-generator": "self.version",
        "helix/server": "self.version",
        "helix/server-sapi": "self.version",
        "helix/session": "self.version",
        "helix/session-http": "self.version",
        "helix/twig-bridge": "self.version",
        "helix/view": "self.version",
        "helix/view-contracts": "self.version"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev",
            "dev-main": "1.0.x-dev"
        }
    },
    "config": {
        "sort-packages": true,
        "platform-check": true,
        "optimize-autoloader": true,
        "allow-plugins": {
            "composer/package-versions-deprecated": true
        }
    },
    "scripts": {
        "dev:merge": "monorepo-builder merge",
        "dev:package-alias": "monorepo-builder package-alias",
        "dev:propagate": "monorepo-builder propagate",
        "dev:validate": "monorepo-builder validate",
        "phpcs": "php-cs-fixer fix --config=.php-cs-fixer.php"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
